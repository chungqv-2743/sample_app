<% provide :title, t(".title") %>
<div class="center jumbotron">
  <h1><%= t ".header" %></h1>
  <h2><%= sanitize t(".content", link: link_to(t(".text"),
    Settings.rails_tutorial)) %>
  </h2>
  <%= link_to t(".signup"), signup_path, class: "btn btn-lg btn-primary" %>
</div>
<%= link_to image_tag("https://download.logo.wine/logo/Ruby_on_Rails/Ruby_on_Rails-Logo.wine.png", alt: "Rails logo", width: "200px"),
                      "https://rubyonrails.org/" %>
